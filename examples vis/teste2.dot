digraph {
  rankdir=LR;
  node [shape = doublecircle,
  		fontcolor=white,
  		color = green,
  		peripheries = 2]; B C;
  node [shape = circle];
  START[label=A];
  START -> START[label="z+x"];
  START -> START[label=a];
  START -> B[label=x];
  B -> C[label=y];
  C -> START[label=z];
  B -> D;
  B -> D[label=x];
  D -> {B; C}
  D -> E[label=x];
}